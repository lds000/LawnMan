	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Resource>
	  <Resource Include="Assets\Gauges\WaterPressureGauge.png">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Resource>
	</ItemGroup>

	<ItemGroup>
	  <Resource Include="Assets\Icons\idle.PNG">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
