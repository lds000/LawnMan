	<ItemGroup>
	  <Resource Include="Assets\Gauges\CompassImage.png">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Resource>
	  <Resource Include="Assets\Gauges\CompassNeedle.svg">
